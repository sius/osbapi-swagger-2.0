{
  "name": "osbapi",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "gen:server:nodejs-server": "swagger-codegen-cli generate -c .project_setup/config/nodejs-server.json -i api/swagger.yaml  -l nodejs-server -o .project_setup/generated/server-nodejs",
    "gen:server:spring-boot": "swagger-codegen-cli generate -c .project_setup/config/spring-boot.json -i api/swagger.yaml  -l spring -o .project_setup/generated/spring-boot",
    "gen:all": "npm run gen:server:nodejs-server && npm run gen:server:spring-boot",
    "setup": "rimraf .project_setup/generated  && mkdir .project_setup/generated && npm run gen:all",
    "editor": "node .project_setup/scripts/editor"
  },
  "author": "sius",
  "license": "ISC",
  "devDependencies": {
    "express": "^4.16.2",
    "global-npm": "^0.3.0",
    "open": "0.0.5",
    "rimraf": "^2.6.2",
    "swagger-editor-dist": "^3.3.0"
  }
}
